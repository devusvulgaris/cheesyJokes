{
  "presets": ["react", "es2015", "stage-0"],
  "env": {
    "development": {
      "presets": ["react-hmre"]
    }
  },
  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties",
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
      }]
  ]
}
